{
    "name": "oro/e2e-tests",
    "type": "symfony-bundle",
    "description": "End-to-End Tests for OroCommerce Application",
    "homepage": "",
    "keywords": ["Oro", "Behat", "E2E"],
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "https://oroinc.com/"
        }
    ],
    "require": {
        "oro/commerce": "7.0.*",
        "oro/commerce-dpd": "7.0.*",
        "oro/crm": "7.0.*",
        "oro/crm-dotmailer": "7.0.*",
        "oro/crm-pro-ldap-bundle": "7.0.*",
        "oro/crm-zendesk": "7.0.*",
        "oro/mailchimp": "7.0.*"
    },
    "autoload": {
        "psr-4": { "Oro\\Bundle\\E2eTestBundle\\": "" },
        "exclude-from-classmap": ["**/Tests/"]
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.oroinc.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        }
    }
}
